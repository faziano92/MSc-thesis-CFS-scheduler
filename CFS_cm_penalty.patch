+static u64 corr_delta_fair(u64 delta, s64 del_CMR, u64 run_del_CMR)
+{
+	unsigned int d1, d2, d3;
+
+	/* If delta_cache_miss_rate of p (num), runnable_delta_cm_rate of cfs_rq (den)
+	   are zero. or if the rate of p is greater or equal than the runnable rate, nothing is to be done */
+	if ((del_CMR == 0)||(run_del_CMR == 0)||(abs(del_CMR) >= run_del_CMR))
+		return delta;
+
+	/* Compute first three decimal digits of ratio */
+	d1 = 10*abs(del_CMR)/run_del_CMR;
+	d2 = 100*abs(del_CMR)/run_del_CMR-10*d1;
+	d3 = 1000*abs(del_CMR)/run_del_CMR-100*d1-10*d2;
+
+	/* Round to the second decimal digit */
+	if (d3 >= 5) {
+		d2++;
+		if (d2 == 10) {
+			d2 = 0;
+			d1++;
+			if (d1 == 10)
+				d1 = 0;
+		}
+	}
+
+	/* Based on the sign of the numerator, add or subtract the percentage */
+	if (del_CMR > 0)
+		delta = delta+(d1*delta)/10+(d2*delta)/100;	
+	else
+		delta = delta-(d1*delta)/10-(d2*delta)/100;
+
+	return delta;
+}
+#endif
